<setup>
		<block title="Enable web proxy" endpoint="configs/conf-vtc" entity="settings">
		<text>
			&lt;i&gt;Setting web proxy url.&lt;/i&gt;
		</text>

		<input field="is_use_proxy">
			<label>Enable proxy</label>
			<type>bool</type>
		</input>

		<input field="http_proxy">
				<label>HTTP Proxy</label>
				<type>text</type>
		</input>

		<input field="https_proxy">
				<label>HTTPS Proxy</label>
				<type>text</type>
		</input>
		</block>
</setup>

